.text ul li,

.text ol li {

	position: relative;

	padding-left: 25px;

}

.text ul li:not(:last-child),

.text ol li:not(:last-child) {

	margin-bottom: 10px;

}

.text ol li {

	padding-left: 45px;

}

.text ul li::before {

	content: "";

	position: absolute;

	top: 16px;

	left: 0;

	width: 7px;

	height: 7px;

	background: linear-gradient(269.02deg, #0042DF -2.76%, #0076E3 43.47%, #009FE3 99.26%);

	-webkit-transform: rotate(45deg);

	-ms-transform: rotate(45deg);

	transform: rotate(45deg);

}

.text ol {

	counter-reset: number;

}

.text ol li::before {

	position: absolute;

	left: 0;

	top: 3px;

	width: 26px;

	height: 26px;

	counter-increment: number;

	content: counter(number);

	font-size: 16px;

	line-height: 26px;

	font-weight: 600;

	color: #52C5FB;

	text-align: center;

}

.text ol li::after {

	content: '';

	position: absolute;

	left: 0;

	top: 3px;

	width: 26px;

	height: 26px;

	border: 3px solid #E6E6E6;

	-webkit-transform: rotate(45deg);

	-ms-transform: rotate(45deg);

	transform: rotate(45deg);

}

